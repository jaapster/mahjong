{
	"parser": "@typescript-eslint/parser",
	"plugins": [
		"@typescript-eslint",
		"@typescript-eslint/eslint-plugin"
	],
	"parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
		"semi": "error"
    }
}

